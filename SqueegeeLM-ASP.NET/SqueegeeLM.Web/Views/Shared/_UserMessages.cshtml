@using SqueegeeLM.Web.Constants

@if (ViewData[MessageConstant.ErrorMessage] != null)
{
    <script>
        $(function() {
            toastr.error('@ViewData[MessageConstant.ErrorMessage]?.ToString()');
        })
    </script>
}

@if (ViewData[MessageConstant.WarningMessage] != null)
{
    <script>
        $(function() {
            toastr.error('@ViewData[MessageConstant.WarningMessage]?.ToString()');
        })
    </script>
}

@if (ViewData[MessageConstant.SuccessMessage] != null)
{
    <script>
        $(function() {
            toastr.error('@ViewData[MessageConstant.SuccessMessage]?.ToString()');
        })
    </script>
}
